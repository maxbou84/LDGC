pip install fuzzywuzzy[speedup]
import pandas as pd 
df = pd.read_excel(r'C:\Users\MAX\Desktop\pythontest.xlsx')
df.head(25)
df[df['Client_EN (Government Contract Pursued) (Standardised) n: 2086'] == 'BOMBARDIER'].shape
df[df['supplier-standardized-name'].str.contains("LOCKHEED.*MARTIN.", na=False)]['supplier-standardized-name'].value_counts()
from fuzzywuzzy import fuzz
from fuzzywuzzy import process
choices = df['supplier-standardized-name'].unique()
choices[:15]
a1=process.extract("Accenture", choices, limit=30, scorer=fuzz.token_set_ratio)
a2=process.extract("Airbus Group", choices, limit=30, scorer=fuzz.token_set_ratio)
a3=process.extract("Alcan ", choices, limit=30, scorer=fuzz.token_set_ratio)
zippedList = list(zip(a1,	a2,	a3))
dfObj = pd.DataFrame(zippedList, columns = ["a1",	"a2",	"a3"])
dfObj.to_excel("lobbytest1.xlsx")
